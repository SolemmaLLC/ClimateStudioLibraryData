<Window x:Class="ClimateStudioLibraryData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClimateStudioLibraryData"
        mc:Ignorable="d"
        Title="MainWindow" Height="416" Width="600">
    <Grid>
        <TextBox Name="ErrorTextBox" HorizontalAlignment="Stretch" TextWrapping="Wrap" Text="no errors..." VerticalAlignment="Stretch" Margin="10,10,10,85" />
        <Button Content="Export" HorizontalAlignment="Stretch"  VerticalAlignment="Bottom"  Click="ExportButton_Click" Height="30" Margin="10,10,10,45"/>
        <Button Content="Import" HorizontalAlignment="Stretch"  VerticalAlignment="Bottom"  Click="ImportButton_Click" Height="30" Margin="10,10,10,10"/>
    </Grid>
</Window>
